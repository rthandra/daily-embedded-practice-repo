#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>

/*
Read each byte of data from an array of 64-bit data in C - MSB and LSB
array = [1,3,4,4,5] - 8 bytes //11000000000000, 330000000000, 
*/
// To execute C, please define "int main()"

int main(){
  uint64_t arr[] = {1,2,3,4};
  int size = sizeof(arr)/sizeof(arr[0]);
  uint8_t *p = (uint8_t *)&arr;

  for(int i = 0; i < size; i++){
   uint8_t lsb = p[i * sizeof(arr[i])]; 
   uint8_t msb = p[i * sizeof(arr[i]) + 7]; //7 bytes

   printf("lsb = %d\n", lsb);
   printf("msb = %d\n", msb);
  }
  return 0;


}
