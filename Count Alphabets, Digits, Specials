
/*
check if alphabets are between the range a to z and A to Z
check if digits are within the range 0 to 9
rest everything else is special

*/
#include <stdio.h>
#include <string.h>

void count(char *s){
  int len = strlen(s);
  int digit = 0;
  int alph = 0;
  int special = 0;

  for (int i = 0; i < len; i++){
    if((s[i] >= 'a' && s[i] <= 'z') || (s[i] >= 'A' && s[i] <= 'Z')){
      alph++;
    }
    else if(s[i] >= '0' && s[i] <= '9'){
      digit++;
    }
    else{
      special++;
    }
  }
  printf("%d %d %d\n", alph, digit, special);

}

int main() {
 char s[] = "aebbcc. 4 77777 eeojf";
 count(s);
}
