#include <stdio.h>
/*range = 1-5

step 1: create a mask for the range
step 2: OR it with num

*/

int setBits(int l, int r, int num){
    int mask = (((1<< (r-l)) - 1) << l);
    num |= mask;
    
    return num;
}


int main()
{
    int num = 0b00101000;
    
    num = setBits(2,7, num);
    /*int bitmask = 0x1E;
    
    printf("bitmask = %#x\n", bitmask);
    num = (num | bitmask);*/
    
    printf("num = %d", num);
    return 0;
}
