#include <stdint.h>
#include <stdbool.h>
#include <stdio.h>

/*

intuition: every number must be divisible exactly by target

[1,2,1,3,2], res = 12
identify numbers thats divisible by res 
[2,4,3,2] - 
2 * 10 = 20 
2 = 20/10 = 2

loop until k:
  first loop i until k:
    second loop j until k:
      if the prod of i and j is divisible of 12:
        store the val in prod var
      else if not div of 12 :
        remove the last added no
      else if prod == 12:
        return true
  return false

  optimal:

  2 * 3 * 2 = 12
  1 = (((12/2)/3)/2
  if its not 1 then there is no product of 12 in the array

*/

bool seive(int *arr, int *n, int *x){
  int res = *x;
  for(int i = 0; i < *n; i++){
    if(*x % arr[i] == 0){
      res /= arr[i];
    }
  }
  if(res == 1){
    return true;
  }
  else{
    return false;
  }

}

/*
bool seive(int *arr, int *n, int *x){
  int prod = 1;

  for(int i = 0; i < *n; i++){
    prod = arr[i];
    for(int j = i + 1; j < *n; j++){

      prod = prod * arr[j];
      if((*x) % prod != 0){
        prod = prod/arr[j];
      }
      else if((*x) % prod == 0 && prod == *x ){
        return true;
      }
    }
  }
  return false;
}
*/
 
int main(){

int arr[] = {2, 3, 2};
int n = 3;
int x = 12;

int res = seive(arr, &n, &x);

printf("res = %d", res);

}
