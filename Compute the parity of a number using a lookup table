int lookup(int x){
    
    x = (x & 0x0000FFFF) ^ (x>>16);
    x = (x & 0x000000FF) ^ (x>>8);
    x = (x & 0x0000000F) ^ (x>>4);
    x = (x & 0x00000003) ^ (x>>2);
    x = (x & 0x00000001) ^ (x>>1);
    
    return x & 1;
}

int main(){
    
    int n = 0;
    scanf("%d", &n);
    
    int parity = lookup(n);
    
    if(parity){
         printf("odd parity");
        
    }
    else{
        printf("even parity");
    }
    return 0;
}
