#include <stdio.h>

/* array of function pointers */

int add (int a, int b){
    return a+b;
}

int sub (int a, int b){
    return a-b;
}

int mul (int a, int b){
    return a*b;
}

int main()
{
    int (*func_ptr[3])(int a, int b) = {add, sub, mul}; //declaring array of function pointers passing two int variables and assinging them to the functions
    
    for(int i = 0; i < 3; i++){
        printf("result = %d\n", func_ptr[i](1,2)); 
    }

    return 0;
}

#define NULL (void*)0
