#include <stdio.h>
#include <math.h>

/*
 3 = 0000 0011 -> 4 
 18 = 0001 0010 -> 32
 
 last set bit's next value's 2's power
 
 step 1: find last set bit
*/
int main()
{
    int n = 33;
    int count = 0;
    
    while( n > 0){
        n = (n >> 1);
        count++;
    }
    
    int power = pow(2, count);
    
    printf("power = %d", power);

    return 0;
}
